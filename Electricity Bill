cname=input("Customer name:-")
unit=eval(input("Enter number of units:-"))
if unit>500:
   print("Charges are:-",3000+20)
elif unit>100 or unit<200:
     print("Charges are:-",500+20)
elif unit>200 or unit<300:
     print("Charges are:-",650+20)
elif unit>300 or unit<500:
     print("Charges are:-",800+20)
else:
    print("Invalid Bill.")
